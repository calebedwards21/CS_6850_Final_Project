    "import pandas as pd \n",
    "import glob\n",
    "\n",
    "path = './data/*.xls*'\n",
    "\n",
    "files = glob.glob(path)\n",
    "\n",
    "dfs = []\n",
    "for file in files: \n",
    "    df = pd.read_excel(file)\n",
    "    dfs.append(df)\n",
    "    \n",
    "df = pd.concat(dfs, ignore_index=True)\n",
    "\n",
    "df"
   ]
